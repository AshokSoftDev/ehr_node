model Patient {
  patient_id   Int      @id @default(autoincrement())
  mrn          String   @unique
  title        String
  firstName    String
  lastName     String
  dateOfBirth  DateTime?
  age          Int?
  gender       String
  mobileNumber String
  address      String
  area         String
  city         String
  state        String
  country      String
  pincode      String
  aadhar       String?
  referalSource String?
  comments     String?
  activeStatus Int      @default(1)
  createdAt    DateTime @default(now())
  createdBy    String
  updatedAt    DateTime @updatedAt
  updatedBy    String
}
